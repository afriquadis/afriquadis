{
  "name": "afriquadis-bilan-express",
  "version": "1.0.0",
  "description": "Application web d'autodiagnostic AFRIQUADIS - Bilan Express - Version Production",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "clean": "if exist .next rmdir /s /q .next && if exist out rmdir /s /q out",
    "production:build": "npm run clean && set NODE_ENV=production && npm run build",
    "production:start": "set NODE_ENV=production && npm run start",
    "production:deploy": "npm run production:build && npm run production:start",
    "docker:build": "docker build -t afriquadis-bilan-express:production .",
    "docker:run": "docker run -p 3000:3000 --env-file env.production afriquadis-bilan-express:production",
    "docker:production": "docker-compose -f docker-compose.production.yml up -d",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "security:audit": "npm audit --audit-level=high",
    "performance:test": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
